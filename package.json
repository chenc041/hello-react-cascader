{
  "name": "hello-react-cascader",
  "version": "0.2.2",
  "description": "a react cascader component",
  "keywords": [
    "react",
    "cascader",
    "antd",
    "component"
  ],
  "main": "dist/lib/hello-react-cascader.js",
  "module": "dist/es/hello-react-cascader.js",
  "typings": "dist/types/index.d.ts",
  "browser": "dist/browser/hello-react-cascader.js",
  "style": "dist/style/index.css",
  "files": [
    "dist"
  ],
  "author": "chenc <double_cl@163.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cWatermelon/hello-react-cascader"
  },
  "bugs": "https://github.com/cWatermelon/hello-react-cascader/issues",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "server": "cross-env NODE_ENV=deveploment webpack-dev-server --process --config webpack.config.js"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.134",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/rollup-plugin-sourcemaps": "^0.4.1",
    "autoprefixer": "^9.6.0",
    "colors": "^1.3.3",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.2.1",
    "lodash.camelcase": "^4.3.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "semantic-release": "^17.2.3",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "sugarss": "^2.0.0",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "antd": "^3.20.0",
    "crypto": "^1.0.1",
    "nanoid": "^2.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
